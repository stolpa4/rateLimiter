{
  "fmt": {
    "files": {
      "include": ["rateLimiter/", "tests/"],
      "exclude": ["tests/coverage/"]
    },
    "options": {
      "useTabs": false,
      "lineWidth": 120,
      "indentWidth": 4,
      "singleQuote": true,
      "proseWrap": "always"
    }
  },
  "tasks": {
    "fmt": "deno fmt",
    "test": "deno test --coverage=tests/coverage",
    "coverage": "deno coverage tests/coverage"
  }
}
